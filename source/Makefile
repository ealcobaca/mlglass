run_clean_data:
	@printf "<<< Running .... >>>\n\n"
	@cd clean_data; python run.py
	@printf "\n\n<<< End .... >>>\n"
run_range:
	@printf "<<< Running .... >>>\n\n"
	@cd range; python run.py --n_cpus=8 -e 1
	@printf "\n\n<<< End .... >>>\n"
run_range_percentil:
	@printf "<<< Running .... >>>\n\n"
	@cd range; python run.py --n_cpus=8 -e 2
	@printf "\n\n<<< End .... >>>\n"
